import java.util.Scanner ;
class Main 
{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("enter a name:");
        String s1=s.next();
        for(int i=s1.length()-1;i>=0;i--)
        {
            char c=s1.charAt(i);
            System.out.println(c);
        }
    }
}
